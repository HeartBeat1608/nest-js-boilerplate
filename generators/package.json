{
  "name": "generator-nest-js-boilerplate",
  "version": "2.12.1",
  "description": "This generator will help you to build your own Nest.js Mongodb API using TypeScript 4",
  "author": "onix-systems",
  "license": "MIT",
  "homepage": "https://onix-systems.com",
  "keywords": [
    "yeoman-generator",
    "typescript",
    "node",
    "nodejs",
    "rest",
    "restfull",
    "api",
    "builder",
    "generator",
    "mongo",
    "ts",
    "starter",
    "mongodb",
    "auth",
    "js",
    "swagger"
  ],
  "repository": {
    "url": "https://github.com/Onix-Systems/nest-js-boilerplate"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "commit": "git-cz"
  },
  "dependencies": {
    "@nestjs/common": "8.3.1",
    "@nestjs/config": "1.2.0",
    "@nestjs/core": "8.3.1",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/mongoose": "9.0.2",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.3.1",
    "@nestjs/swagger": "5.2.0",
    "@nestjs/testing": "8.3.1",
    "@nestjs/typeorm": "8.0.3",
    "bcryptjs": "2.4.3",
    "chalk": "5.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "connect-flash": "0.1.1",
    "connect-mongodb-session": "3.1.1",
    "connect-redis": "6.1.1",
    "cookie-parser": "1.4.6",
    "express": "4.17.3",
    "express-handlebars": "6.0.2",
    "express-mysql-session": "2.1.7",
    "express-session": "1.17.2",
    "hbs": "4.2.0",
    "husky": "7.0.4",
    "ioredis": "4.28.5",
    "jsonapi-serializer": "3.6.7",
    "module-alias": "2.2.2",
    "mongoose": "6.2.2",
    "nestjs-redis": "git+https://github.com/GyanendroKh/nestjs-redis.git#2d00f67c",
    "open": "8.4.0",
    "passport": "0.4.1",
    "passport-google-oauth20": "2.0.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "print-message": "3.0.1",
    "redis": "4.0.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.4",
    "swagger-ui-express": "4.3.0",
    "ts-node": "10.5.0",
    "typeorm": "0.2.43",
    "yeoman-generator": "5.6.1",
    "yosay": "2.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.1",
    "@nestjs/schematics": "8.0.6",
    "@types/connect-flash": "0.0.37",
    "@types/connect-redis": "0.0.18",
    "@types/express": "4.17.13",
    "@types/express-handlebars": "6.0.0",
    "@types/express-session": "1.17.4",
    "@types/jest": "27.4.1",
    "@types/jsonapi-serializer": "3.6.5",
    "@types/node": "17.0.20",
    "@types/passport-google-oauth20": "2.0.11",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/redis": "4.0.11",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.9.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "2.25.4",
    "jest": "27.5.1",
    "prettier": "2.5.1",
    "supertest": "6.2.2",
    "ts-jest": "27.1.3",
    "ts-loader": "9.2.6",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
